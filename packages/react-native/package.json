{
  "name": "@mitter-io/react-native",
  "version": "0.6.47",
  "description": "",
  "keywords": [],
  "main": "dist/mitter-react-native.umd.js",
  "module": "dist/mitter-react-native.es5.js",
  "typings": "dist/types/mitter-react-native.d.ts",
  "files": [
    "dist"
  ],
  "author": "Vineeth George <vineeth@mitter.io>",
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "ac": "tsc",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts && typedoc --out docs --target es6 --theme minimal --mode file src",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:prod": "npm run lint && npm run test -- --coverage --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "precommit": "lint-staged",
    "prepare": "yarn run build",
    "prepublishOnly": "./../../ci-scripts/publish-tsdocs.sh"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^7.1.2",
    "@commitlint/config-conventional": "^7.1.2",
    "@types/base-64": "^0.1.2",
    "@types/jest": "^23.3.2",
    "@types/nanoid": "^2.0.0",
    "@types/react-native": "^0.57.7",
    "@types/react-native-uuid": "^1.4.0",
    "@types/rn-fetch-blob": "^1.2.1",
    "colors": "^1.3.2",
    "commitizen": "^3.0.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.0.1",
    "jest": "^23.6.0",
    "jest-config": "^23.6.0",
    "lint-staged": "^7.3.0",
    "lodash.camelcase": "^4.3.0",
    "prettier": "^1.14.3",
    "prompt": "^1.0.0",
    "react-native": "^0.57.7",
    "react-native-firebase": "^5.1.1",
    "replace-in-file": "^3.4.2",
    "rimraf": "^2.6.2",
    "rn-fetch-blob": "0.10.13",
    "rollup": "^0.66.2",
    "rollup-plugin-commonjs": "^9.0.0",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.2.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.17.0",
    "s3-publish": "^1.0.11",
    "semantic-release": "^15.0.0",
    "ts-jest": "^23.10.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.14.2",
    "typescript": "3.0.3",
    "validate-commit-msg": "^2.12.2"
  },
  "dependencies": {
    "@mitter-io/core": "^0.6.47",
    "@mitter-io/models": "^0.6.47",
    "axios": "^0.18.0",
    "base-64": "^0.1.0",
    "nanoid": "^2.0.3",
    "react-native-base64": "^0.0.2",
    "react-native-uuid": "^1.4.9",
    "rollup-plugin-babel": "^3.0.7",
    "whatwg-fetch": "^2.0.4"
  },
  "gitHead": "22589312118f9c486bbd8a7007182a2f2826ef34"
}
